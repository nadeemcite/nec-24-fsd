{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "outDir": "./out",
        "rootDir": "./src",
        "strict": true,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strictPropertyInitialization": false
    },
    "include":[
        "src"
    ],
    "exclude":[
        "src/**/*test.ts"
    ]
}